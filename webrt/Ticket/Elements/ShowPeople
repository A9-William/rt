	  <& /Elements/ShadedBox, title => 'Owner' , content => $Ticket->OwnerObj->UserId &>
	  <& /Elements/ShadedBox, title => 'Requestors' , content => $Ticket->RequestorsAsString &>
	  <& /Elements/ShadedBox, title => 'Cc' , content => $Ticket->CcAsString &>
	  <& /Elements/ShadedBox, title => 'AdminCc' , content => $Ticket->AdminCcAsString &>

<%ARGS>
$Ticket => undef
</%ARGS>

