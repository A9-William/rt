%# $Header$

<SELECT NAME ="<%$Name%>">
% while (my $queue=$q->Next) {
<OPTION VALUE="<%$queue->Id%>"><%$queue->Description%></OPTION>
% }
</SELECT>

<%ARGS>
$Name => undef
$True => undef
$False => undef
$Default => undef
</%ARGS>

<%INIT>
# TODO handle Default
use RT::Queues;
my $q=new RT::Queues;
$q->UnLimit;
</%INIT>
