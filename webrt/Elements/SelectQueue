%# $Header$

<SELECT NAME ="<%$Name%>">
<OPTION VALUE="">-</OPTION>
% while (my $queue=$q->Next) {
<OPTION VALUE="<%$queue->Id%>" <%($queue->Id eq $Default) && 'DEFAULT'%>><%$queue->QueueId%> (<%$queue->Description%>)</OPTION>
% }
</SELECT>

<%ARGS>
$Name => undef
$Default => undef
</%ARGS>

<%INIT>
use RT::Queues;
my $q=new RT::Queues($session{'CurrentUser'});
$q->UnLimit;
</%INIT>
