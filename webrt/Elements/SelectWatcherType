<SELECT NAME ="WType">
<OPTION VALUE="Requestor" <%$rsel%>>Requestor
<OPTION VALUE="Cc" <%$csel%>>Cc
<OPTION VALUE="AdminCc" <%$asel%>>AdminCc
</SELECT>

<%ARGS>
$Default=>undef
</%ARGS>
<%INIT>
my $rsel=($Default eq "Requestor")&&"SELECTED";
my $csel=($Default eq "Cc")&&"SELECTED";
my $asel=($Default eq "AdminCc")&&"SELECTED";
</%INIT>
