<SELECT NAME="<%$Name%>">

%while ( my $User = $Users->Next()) {

<OPTION VALUE="<%$User->Id()%>"><%$User->UserId() %></OPTION>

%}

</SELECT>

<%ARGS>

$Name => undef
</%ARGS>

<%INIT>
use RT::CurrentUser;
use RT::Users;
my $CurrentUser = RT::CurrentUser->new(2);
my $Users = RT::Users->new($CurrentUser);
$Users->Limit( FIELD => 'id',
		OPERATOR => "!=",
	        VALUE => 1
		);
</%INIT>