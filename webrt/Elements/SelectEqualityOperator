%# $Header: /raid/cvsroot/rt/webrt/Elements/SelectEqualityOperator,v 1.1 2001/11/29 08:50:56 jesse Exp $
<SELECT NAME ="<%$Name%>">
% while (my $option = shift @Options) {
% my $value = shift @Values;
<OPTION VALUE="<%$value%>"
% if ($Default eq '$value') {
SELECTED
% }
><%$option%></OPTION>
% }
</SELECT>

<%ARGS>
$Name => undef
@Options => ('less than', 'equal to', 'greater than', 'not equal to')
@Values => qw(< = > !=)
$Default => undef
</%ARGS>
