%# $Header$

<SELECT NAME ="<%$Name%>">
<OPTION VALUE="">-</OPTION>
%foreach my $status (@status) {
<OPTION VALUE="<%$status%>" <%($Default eq $status) && 'SELECTED'%>><%$status%></OPTION>
% }
</SELECT>
<%ONCE>
my @status = qw(new open stalled resolved dead);
</%ONCE>
<%ARGS>
$Name => undef
$Default => undef
 
</%ARGS>
