<ul>
% while (my $v = $values->Next) {
<li>
<input type="checkbox" name="CustomField-<%$CustomField->Id%>-DeleteValue" value="<%$v->id%>">
<%$v->Name%>
<i>(<%$v->Description%>)</i>
