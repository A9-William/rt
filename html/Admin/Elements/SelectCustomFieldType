<SELECT NAME ="<%$Name%>">
%for my $option (RT::CustomField->Types) {
<OPTION VALUE="<%$option%>" <%$option eq $Default && "SELECTED"%>><%$option%></OPTION>
%}
</SELECT>

<%ARGS>
$Default=>undef
$Name => 'Type'
</%ARGS>
