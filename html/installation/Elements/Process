<%init>
for my $type ( @$Types ) {
    $RT::Installer->{InstallConfig}{$type} =
        $m->comp( $RT::Installer->{Option}{$type}{Widget} . ':Process', Name =>
                $type, Arguments => $Arguments );
}
$RT::Installer->{TimeStamp} = time; # need this to force %session update
</%init>

<%args>
$Arguments
$Types
</%args>
