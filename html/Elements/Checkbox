%# $Header: /raid/cvsroot/rt/webrt/Elements/Checkbox,v 1.2 2001/11/06 23:06:57 jesse Exp $
<INPUT TYPE="Checkbox" NAME ="<%$Name%>" <%$IsChecked%>>

<%ARGS>
$Name => undef
$Default => undef
$True => undef
$False => undef
$IsChecked => undef
</%ARGS>

<%INIT>
$IsChecked = 
  ($Default && $Default =~ /checked/i)
    ? " CHECKED " : "";
1;
</%INIT>
